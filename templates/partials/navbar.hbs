                --h1-font: 3.8rem;
                --h2-font: 3rem;
                --h4-font: 1.8rem;
                --p-font: 1.02rem;
            }

        }

        @media (max-width:900px) {
            .about {
                flex-direction: column;
            }

            .menu-items {
                flex-direction: column;
            }

            .home-text {
                width: 40rem;
                padding: 4rem 2rem 1rem;
            }

            .home-text h1 {
                letter-spacing: .03rem;
                line-height: 90px;
                font-size: var(--h1-font);
            }

            .footer-links {
                align-items: center;
                flex-direction: column;
            }

            :root {
                --h1-font: 3.6rem;
                --h2-font: 2.8rem;
                --h4-font: 1.7rem;
                --p-font: 1.02rem;
            }
        }

        @media (max-width:650px) {
            .navbar {
                padding: 1rem;
            }

            .navbar-login {
                display: none;
            }

            .navbar-logo img {
                width: 110px;
            }

            .home-text {
                width: 30rem;
                padding: 3rem 2rem 1rem;
            }

            .home-text h1 {
                letter-spacing: .03rem;
                line-height: 4.6rem;
                font-size: var(--h1-font);
            }

            .home-text p {
                line-height: 1.3rem;
            }

