                --h1-font: 3.2rem;
                --h2-font: 2.4rem;
                --h4-font: 1.3rem;
                --p-font: 1rem;
            }

            .section {
                padding: 0 1rem;
            }
        }

        @media (max-width:550px) {
            .home-text h1 {
                letter-spacing: .02rem;
                line-height: 3.5rem;
                font-size: var(--h1-font);
            }

            :root {
                --h1-font: 2.6rem;
                --h2-font: 2rem;
                --h4-font: 1.1rem;
                --p-font: 1.01rem;
            }

            .home {
                padding-left: 1rem;
            }
        }
