            }

            .knife img {
                height: 800px;
            }

            .about-us p,
            .our-history p {
                margin: 3rem 0;
            }
        }

        @media (max-width:1150px) {
            .navbar-links {
                display: none;
            }

            .bar {
                display: block;
            }

            .home-text h1 {
                letter-spacing: .03rem;
                line-height: 90px;
                font-size: var(--h1-font);
            }

        }

        @media (max-width:1050px) {
            .home {
                background-image: url(home-bg2.png);
                background-repeat: no-repeat;
                background-size: cover;
                height: auto;
            }

